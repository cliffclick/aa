// Type: *[8]( _, *[2]( _, int64, str), *[2]( _, int64, str))
// Eval: *[8]( _, *[2]( _, 124, "litered"), *[2]( _, 457, "liteblue"))
color = { hex name -> (pair hex name )};
red  = (color 123 "red" );
blue = (color 456 "blue");
lite = { c -> (color (+1 c._) (s+ "lite" c._))};
(pair (lite red) (lite blue))
