// Type: *[]( _, *[]( _, int64, str), *[]( _, int64, str))
// Eval: *[]( _, *[]( _, 124, "litered"), *[]( _, 457, "liteblue"))
color = { hex name -> (pair hex name )};
red  = (color 123 "red" );
blue = (color 456 "blue");
lite = { c -> (color (+1 c._) (s+ "lite" c._))};
(pair (lite red) (lite blue))
