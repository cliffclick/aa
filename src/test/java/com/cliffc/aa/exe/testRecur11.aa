// NON-Polymorphic records; see testRecur10.aa
// Type: ( A:( @{ x=nint8; }, nint8), A)
// Eval: ( ( @{ x=3; y=4; }, 3), ( @{ x=4; z=@{ q=1; }; }, 4))
( { fun ->
     (pair (fun @{x =3; y =4       ;} )   // available {x,y}
           (fun @{x =4; z =@{q =1;};} ) ) // available {x,z}
  }
  { rec -> (pair rec rec.x) }  // required  {x}
) 
