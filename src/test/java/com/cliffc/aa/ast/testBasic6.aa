// Mutually recursive
// GCP: *[22]@{ _; someC=PA:*[]@{^=$[5,6](...); _=%[5,6][]; $nil}?; someD=PA; ...}
// HMT: *[22]@{ _; someC=flt:3.14; someD=int:nint8; }
A = { x -> E() ? B(x) : x };
D = {   -> E() ? B(1) : C(2) };
C = { x -> A(x) };
B = { x -> C(x) };
E = {   -> math.rand(2) };
@{ someD=D(); someC=C(3.14); }
